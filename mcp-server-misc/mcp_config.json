{
  "name": "Web Context MCP Server",
  "version": "1.0.0",
  "description": "Standalone MCP server for web documentation context. Works with any IDE/Editor.",
  "standalone": true,
  "server": {
    "transport": "stdio",
    "host": "127.0.0.1",
    "port": 3000,
    "sse_port": 3001
  },
  "context_sets_file": "web_context_sets.json",
  "cache": {
    "enabled": true,
    "directory": "cache",
    "ttl_hours": 24
  },
  "logging": {
    "level": "INFO",
    "stderr": true,
    "file": null
  }
}
